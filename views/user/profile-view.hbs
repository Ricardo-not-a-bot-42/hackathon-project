<div class="container">
  <img src="{{ userSingle.photo }}" alt="">
  <h1>{{ userSingle.name }}, {{userSingle.age}}</h1>
  <h3>My gender:</h3>
  <h3>{{userSingle.gender}}</h3>
  <h3>Preference: </h3>
  {{ userSingle.orientation}}
  <h3>Profession: </h3>
  {{userSingle.profession}}
  <h3>Disabilities: </h3>
  {{#each userSingle.disabilities}}
  <h3>{{this}}</h3>
  {{/each}}
  <h3>About me: </h3>
</div>

{{#if isOwner}}
{{/if}}